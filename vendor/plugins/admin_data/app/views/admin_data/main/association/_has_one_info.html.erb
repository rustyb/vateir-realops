<% if AdminData::Util.has_one_what(klass.name).size > 0 %>
  <p>
    <strong>has_one :</strong> 

    <% admin_data_has_one_what(klass.name).each do |bt| %>
      &nbsp;      
      <% if  model.send(bt) %>
        <%= link_to bt, admin_data_show_path(:klass => admin_data_get_belongs_to_class(model,bt), 
                                              :model_id => model.send(bt).id) %>        
      <% else %>
          <%= bt %>              
      <% end %>  
    <% end %>  
  </p> 
<% end %>  
